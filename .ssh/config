ControlMaster auto
ControlPath ~/.ssh/control:%h:%p:%r

Host localhost
	HostName 127.0.0.1
	ForwardAgent yes
	IdentityFile ~/.ssh/testls1043ardb


Host ci
    RemoteCommand zsh -l

Host ci-tunnel
    RemoteCommand zsh -l
    # squid service on local port 3129
    # http_proxy on remote port 3129
    RemoteForward 3129 127.0.0.1:3128

Host raw-ci ci ci-tunnel
    User ci
    HostName 192.168.10.8
    RequestTTY force


Host nanopi-ci-tunnel
    # squid service on local port 3129
    # http_proxy on remote port 3129
    RemoteForward 3129 127.0.0.1:3128

Host nanopi-ci nanopi-ci-tunnel
    User pi
    HostName 192.168.64.2
    ProxyJump raw-ci


Host ls1043ardb
    ProxyJump nanopi-ci

Host ls1043ardb 192.168.18.6
    User root
    HostName 192.168.18.6
    StrictHostKeyChecking no
