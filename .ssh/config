Host localhost
	HostName 127.0.0.1
	ForwardAgent yes
	IdentityFile ~/.ssh/testls1043ardb

Host ci-tunnel
    User ci
    HostName 192.168.32.1
    RequestTTY force
    RemoteCommand zsh -l
    # squid service on local port 3129
    # http_proxy on remote port 3129
    RemoteForward 3129 127.0.0.1:3128

Host ci
    User ci
    HostName 192.168.32.1
    RequestTTY force
    RemoteCommand zsh -l

Host raw-ci
    User ci
    HostName 192.168.32.1
    RequestTTY force
